<p id="notice"><%= notice %></p>
<div class="container" >
  <h1>Users</h1>

  <table class="table">
    <thead>
      <tr>
        <th>Name</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @users.each do |user| %>
        <tr>
          <th scope="col"><%= link_to user.name, user  %></th>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>

<br>
