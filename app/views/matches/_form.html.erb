<div class="container">
<%= form_with(model: match, local: true) do |form| %>
  <% if match.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(match.errors.count, "error") %> prohibited this match from being saved:</h2>

      <ul>
      <% match.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :user_id, t('teacher') %>
    <%= form.number_field :user_id, class:"form-control" %>
  </div>

  <div class="field">
    <%= form.label :student_id, t('student') %>
    <%= form.number_field :student_id, class:"form-control" %>
  </div><br/><br/>

  <div class="actions">
    <%= form.submit t('create_match'), class:"btn btn-primary" %>
  </div>
<% end %>
</div>
